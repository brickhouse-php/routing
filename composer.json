{
    "name": "brickhouse/routing",
    "description": "Routing mechanism for Brickhouse applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "routing",
        "framework",
        "brickhouse",
        "mvc"
    ],
    "homepage": "https://github.com/maxnatamo/brickhouse",
    "support": {
        "issues": "https://github.com/maxnatamo/brickhouse/issues",
        "source": "https://github.com/maxnatamo/brickhouse"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Max T. Kristiansen",
            "email": "me@maxtrier.dk",
            "role": "Author",
            "homepage": "https://maxtrier.dk"
        }
    ],
    "scripts": {
        "bench": "phpbench run --report=default",
        "bench:compare": "phpbench run --report=micro-compare --group compare"
    },
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "pestphp/pest": "^3.7",
        "phpbench/phpbench": "^1.3",
        "pestphp/pest-plugin-type-coverage": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Brickhouse\\Routing\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Brickhouse\\Routing\\Tests\\": "tests/",
            "Brickhouse\\Routing\\Benchmarks\\": "bench/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
